{
  "name": "n8n-nodes-filemaker-odata",
  "version": "0.1.0",
  "description": "n8n community node for FileMaker OData API - Query, create, update, and delete records in FileMaker databases",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "filemaker",
    "odata",
    "database",
    "claris",
    "otto",
    "ottofms"
  ],
  "license": "MIT",
  "homepage": "https://github.com/proofgeist/fmodata",
  "author": {
    "name": "Proofgeist",
    "email": "support@proofgeist.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/proofgeist/fmodata.git"
  },
  "main": "dist/nodes/FileMakerOData/FileMakerOData.node.js",
  "scripts": {
    "build": "tsc && npm run copy-icons",
    "copy-icons": "copyfiles -u 0 \"nodes/**/*.svg\" dist/",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/FileMakerODataApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/FileMakerOData/FileMakerOData.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "copyfiles": "^2.4.1",
    "n8n-workflow": "^1.22.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}

