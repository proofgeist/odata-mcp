{
	"name": "n8n-nodes-filemaker-odata",
	"version": "0.2.5",
	"description": "n8n community node for FileMaker OData API - Query, create, update, and delete records in FileMaker databases",
	"license": "MIT",
	"homepage": "https://github.com/proofgeist/fmodata",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"filemaker",
		"odata",
		"database",
		"claris",
		"ottofms"
	],
	"author": {
		"name": "Proofgeist",
		"email": "support@proofgeist.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/proofgeist/fmodata.git"
	},
	"scripts": {
		"build": "n8n-node build && npm run copy-json",
		"build:watch": "tsc --watch",
		"copy-json": "copyfiles -u 0 \"nodes/**/*.json\" dist/",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"prepublishOnly": "npm run build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/FileMakerODataApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/FileMakerOData/FileMakerOData.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "^0.17.0",
		"copyfiles": "^2.4.1",
		"n8n-workflow": "^1.22.0",
		"typescript": "^5.3.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
